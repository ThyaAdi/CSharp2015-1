    <h2>Account</h2>
    <p>
        <a href="#/Account/Create">Create New</a>
    </p>
    <table class="table">
        <thead>
        <tr>
            <th>
                Transactions
            </th>
            <th>
                Name
            </th>
            <th>
                Institution
            </th>
            <th>
                Business
            </th>
            <th></th>
        </tr>
        </thead>
        <tbody>
        <tr ng-repeat="item in data">
            <td>
                    <a ng-href="#Transaction?Account={{item.Id }}" >
                    {{item.Transactions_Count}}
                    </a>
            </td>
            <td>
                {{ item.Name }}
            </td>
            <td>
                {{ item.Institution }}
            </td>
            <td>
                {{ item.Business }}
            </td>
            <td>
                <a ng-href="#/Account/Edit/{{item.Id}}">Edit</a> |
                <a ng-href="#/Account/{{item.Id}}">Details</a> |
                <a ng-href="#/Account/Delete/{{item.Id}}">Delete</a>
            </td>
        </tr>
        </tbody>
    
    </table>
