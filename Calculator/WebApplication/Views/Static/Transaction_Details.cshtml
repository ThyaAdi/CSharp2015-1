    <h2>Transaction Details Test</h2>
        <div class="form-horizontal">
            <ul id="validationSummary"></ul>
            <div class="form-group">
                <label for="Account" class = "control-label col-md-2">
                 Account
                </label>
                <div class="col-md-10">
                    <a ng-href="#Account/{{Account }}" >
                    {{data.Account_Name}}
                    </a>
                </div>
            </div>
    
            <div class="form-group">
                <label for="Date" class = "control-label col-md-2">
                Date
                </label>
                <div class="col-md-10">
                    {{data.Date}}
                </div>
            </div>
    
            <div class="form-group">
                <label for="Payee" class = "control-label col-md-2">
                Payee
                </label>
                <div class="col-md-10">
                    {{data.Payee}}
                </div>
            </div>
    
            <div class="form-group">
                <label for="Amount" class = "control-label col-md-2">
                Amount
                </label>
                <div class="col-md-10">
                    {{data.Amount}}
                </div>
            </div>
    
            <div class="form-group">
                <label for="Tag" class = "control-label col-md-2">
                Tag
                </label>
                <div class="col-md-10">
                    {{data.Tag}}
                </div>
            </div>
    
           
        </div>
    
    <div>
        <a href="#/Transaction">Back to List</a>
    </div>
